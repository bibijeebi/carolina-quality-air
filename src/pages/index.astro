---
import Layout from '../layouts/Layout.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';

const stats = [
  { value: "20,000+", label: "Systems Cleaned" },
  { value: "25+", label: "Years Experience" },
  { value: "37", label: "Team Members" },
];

const services = [
  {
    title: "Residential",
    description: "Whole-home duct cleaning for healthier indoor air. Perfect for families, allergy sufferers, and pet owners.",
    icon: "home"
  },
  {
    title: "Commercial",
    description: "Office buildings, retail spaces, and restaurants. Keep employees and customers breathing clean.",
    icon: "building"
  },
  {
    title: "Industrial",
    description: "Manufacturing plants, warehouses, and large facilities with complex HVAC systems.",
    icon: "factory"
  },
  {
    title: "Medical",
    description: "Hospitals, clinics, and healthcare facilities requiring the highest air quality standards.",
    icon: "medical"
  }
];

const benefits = [
  { title: "Reduce Allergies", description: "Remove dust, pollen, pet dander, and other allergens from your air system." },
  { title: "Eliminate Odors", description: "Get rid of musty smells, smoke residue, and stale air from your ducts." },
  { title: "Improve Airflow", description: "Clean ducts mean better HVAC efficiency and lower energy bills." },
];


---

<Layout title="Home">
  <Header />
  
  <main class="pt-20">
    <!-- Hero Section -->
    <section class="relative min-h-[90vh] flex items-center overflow-hidden">
      <!-- Background pattern -->
      <div class="absolute inset-0 bg-gradient-to-br from-slate-50 via-white to-brand-50"></div>
      <div class="absolute inset-0 opacity-[0.03]" style="background-image: url('data:image/svg+xml,%3Csvg width=\'60\' height=\'60\' viewBox=\'0 0 60 60\' xmlns=\'http://www.w3.org/2000/svg\'%3E%3Cg fill=\'none\' fill-rule=\'evenodd\'%3E%3Cg fill=\'%23000000\' fill-opacity=\'1\'%3E%3Cpath d=\'M36 34v-4h-2v4h-4v2h4v4h2v-4h4v-2h-4zm0-30V0h-2v4h-4v2h4v4h2V6h4V4h-4zM6 34v-4H4v4H0v2h4v4h2v-4h4v-2H6zM6 4V0H4v4H0v2h4v4h2V6h4V4H6z\'/%3E%3C/g%3E%3C/g%3E%3C/svg%3E');"></div>
      
      <div class="relative max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-20">
        <div class="grid lg:grid-cols-2 gap-12 lg:gap-20 items-center">
          <!-- Left content -->
          <div class="animate-fade-up">
            <div class="inline-flex items-center gap-2 px-4 py-2 bg-emerald-50 text-emerald-700 text-sm font-medium rounded-full mb-6">
              <svg class="w-4 h-4" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M6.267 3.455a3.066 3.066 0 001.745-.723 3.066 3.066 0 013.976 0 3.066 3.066 0 001.745.723 3.066 3.066 0 012.812 2.812c.051.643.304 1.254.723 1.745a3.066 3.066 0 010 3.976 3.066 3.066 0 00-.723 1.745 3.066 3.066 0 01-2.812 2.812 3.066 3.066 0 00-1.745.723 3.066 3.066 0 01-3.976 0 3.066 3.066 0 00-1.745-.723 3.066 3.066 0 01-2.812-2.812 3.066 3.066 0 00-.723-1.745 3.066 3.066 0 010-3.976 3.066 3.066 0 00.723-1.745 3.066 3.066 0 012.812-2.812zm7.44 5.252a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
              </svg>
              NADCA Certified Since 2000
            </div>
            
            <h1 class="text-5xl sm:text-6xl lg:text-7xl font-bold text-slate-900 leading-[1.1] mb-6">
              Breathe 
              <span class="text-brand-600">cleaner</span>
              <br />air at home.
            </h1>
            
            <p class="text-xl text-slate-600 mb-8 max-w-lg leading-relaxed">
              Eastern North Carolina's first NADCA-certified air duct cleaning experts. 
              Family owned since 1999, trusted by hospitals, universities, and 20,000+ homes.
            </p>
            
            <div class="flex flex-col sm:flex-row gap-4 mb-12">
              <a 
                href="/contact" 
                class="inline-flex items-center justify-center gap-2 px-8 py-4 bg-brand-600 text-white font-semibold rounded-full hover:bg-brand-700 transition-all hover:shadow-xl hover:shadow-brand-600/25 hover:-translate-y-0.5"
              >
                Get Free Quote
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3" />
                </svg>
              </a>
              <a 
                href="tel:+12523217447"
                class="inline-flex items-center justify-center gap-2 px-8 py-4 bg-white text-slate-900 font-semibold rounded-full border-2 border-slate-200 hover:border-brand-600 hover:text-brand-600 transition-all"
              >
                <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z" />
                </svg>
                252-321-7447
              </a>
            </div>

            <!-- Stats -->
            <div class="flex flex-wrap gap-8">
              {stats.map(stat => (
                <div>
                  <div class="text-3xl font-bold text-slate-900">{stat.value}</div>
                  <div class="text-sm text-slate-500">{stat.label}</div>
                </div>
              ))}
            </div>
          </div>

          <!-- Right side - Before/After visual -->
          <div class="relative animate-fade-up delay-200">
            <div class="relative rounded-3xl overflow-hidden shadow-2xl">
              <!-- Before/After comparison -->
              <div class="aspect-[4/3] relative">
                <div class="absolute inset-0 flex">
                  <!-- Before side -->
                  <div class="w-1/2 relative overflow-hidden">
                    <img src="/images/before-after/3-before.jpg" alt="Dirty duct before cleaning" class="absolute inset-0 w-full h-full object-cover" />
                    <div class="absolute inset-0 bg-black/20"></div>
                    <span class="absolute top-4 left-4 px-3 py-1.5 bg-black/60 text-white text-sm font-medium rounded-full">Before</span>
                  </div>
                  <!-- After side -->
                  <div class="w-1/2 relative overflow-hidden">
                    <img src="/images/before-after/3-after.jpg" alt="Clean duct after cleaning" class="absolute inset-0 w-full h-full object-cover" />
                    <span class="absolute top-4 right-4 px-3 py-1.5 bg-brand-600 text-white text-sm font-medium rounded-full">After</span>
                  </div>
                </div>
                <!-- Divider line -->
                <div class="absolute inset-y-0 left-1/2 w-1 bg-white shadow-lg transform -translate-x-1/2"></div>
                <div class="absolute top-1/2 left-1/2 transform -translate-x-1/2 -translate-y-1/2 w-10 h-10 bg-white rounded-full shadow-lg flex items-center justify-center">
                  <svg class="w-5 h-5 text-slate-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 9l4-4 4 4m0 6l-4 4-4-4" />
                  </svg>
                </div>
              </div>
            </div>
            
            <!-- Floating badge -->
            <div class="absolute -bottom-4 -left-4 bg-white rounded-2xl shadow-xl p-4 animate-fade-up delay-400">
              <div class="flex items-center gap-3">
                <div class="w-12 h-12 bg-emerald-100 rounded-xl flex items-center justify-center">
                  <svg class="w-6 h-6 text-emerald-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z" />
                  </svg>
                </div>
                <div>
                  <div class="text-sm font-semibold text-slate-900">EPA Approved</div>
                  <div class="text-xs text-slate-500">Source Removal Method</div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- Services Section -->
    <section class="py-24 bg-white">
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center max-w-2xl mx-auto mb-16">
          <span class="text-brand-600 font-semibold text-sm uppercase tracking-wider">Our Services</span>
          <h2 class="text-4xl sm:text-5xl font-bold text-slate-900 mt-3 mb-4">
            Clean air for every space
          </h2>
          <p class="text-lg text-slate-600">
            From single-family homes to hospital operating rooms, we have the equipment and expertise.
          </p>
        </div>

        <div class="grid sm:grid-cols-2 lg:grid-cols-4 gap-6">
          {services.map((service, i) => (
            <a 
              href="/services" 
              class="group relative bg-slate-50 rounded-2xl p-8 hover:bg-brand-600 transition-all duration-300 hover:shadow-xl hover:shadow-brand-600/20 hover:-translate-y-1"
            >
              <div class="w-14 h-14 bg-brand-100 rounded-xl flex items-center justify-center mb-6 group-hover:bg-white/20 transition-colors">
                {service.icon === "home" && (
                  <svg class="w-7 h-7 text-brand-600 group-hover:text-white transition-colors" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 12l2-2m0 0l7-7 7 7M5 10v10a1 1 0 001 1h3m10-11l2 2m-2-2v10a1 1 0 01-1 1h-3m-6 0a1 1 0 001-1v-4a1 1 0 011-1h2a1 1 0 011 1v4a1 1 0 001 1m-6 0h6" />
                  </svg>
                )}
                {service.icon === "building" && (
                  <svg class="w-7 h-7 text-brand-600 group-hover:text-white transition-colors" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4" />
                  </svg>
                )}
                {service.icon === "factory" && (
                  <svg class="w-7 h-7 text-brand-600 group-hover:text-white transition-colors" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19.428 15.428a2 2 0 00-1.022-.547l-2.387-.477a6 6 0 00-3.86.517l-.318.158a6 6 0 01-3.86.517L6.05 15.21a2 2 0 00-1.806.547M8 4h8l-1 1v5.172a2 2 0 00.586 1.414l5 5c1.26 1.26.367 3.414-1.415 3.414H4.828c-1.782 0-2.674-2.154-1.414-3.414l5-5A2 2 0 009 10.172V5L8 4z" />
                  </svg>
                )}
                {service.icon === "medical" && (
                  <svg class="w-7 h-7 text-brand-600 group-hover:text-white transition-colors" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4.318 6.318a4.5 4.5 0 000 6.364L12 20.364l7.682-7.682a4.5 4.5 0 00-6.364-6.364L12 7.636l-1.318-1.318a4.5 4.5 0 00-6.364 0z" />
                  </svg>
                )}
              </div>
              <h3 class="text-xl font-bold text-slate-900 group-hover:text-white mb-3 transition-colors">
                {service.title}
              </h3>
              <p class="text-slate-600 group-hover:text-white/80 transition-colors text-sm leading-relaxed">
                {service.description}
              </p>
              <div class="mt-6 flex items-center text-brand-600 group-hover:text-white font-medium text-sm transition-colors">
                Learn more
                <svg class="w-4 h-4 ml-2 group-hover:translate-x-1 transition-transform" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3" />
                </svg>
              </div>
            </a>
          ))}
        </div>
      </div>
    </section>

    <!-- Why Clean Section -->
    <section class="py-24 bg-slate-900 text-white relative overflow-hidden">
      <!-- Background decoration -->
      <div class="absolute top-0 left-0 w-96 h-96 bg-brand-600/10 rounded-full blur-3xl"></div>
      <div class="absolute bottom-0 right-0 w-96 h-96 bg-brand-600/10 rounded-full blur-3xl"></div>
      
      <div class="relative max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="grid lg:grid-cols-2 gap-16 items-center">
          <div>
            <span class="text-brand-400 font-semibold text-sm uppercase tracking-wider">Why It Matters</span>
            <h2 class="text-4xl sm:text-5xl font-bold mt-3 mb-6">
              Indoor air can be 70x more polluted than outdoor air
            </h2>
            <p class="text-lg text-slate-300 mb-8 leading-relaxed">
              Most people spend 60-90% of their time indoors. Your HVAC system circulates air through the same dirty ducts 
              every day, spreading dust, allergens, mold spores, and bacteria throughout your space.
            </p>
            
            <div class="space-y-6 mb-10">
              {benefits.map(benefit => (
                <div class="flex gap-4">
                  <div class="flex-shrink-0 w-10 h-10 bg-brand-600 rounded-lg flex items-center justify-center">
                    <svg class="w-5 h-5 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
                    </svg>
                  </div>
                  <div>
                    <h4 class="font-semibold text-white mb-1">{benefit.title}</h4>
                    <p class="text-slate-400 text-sm">{benefit.description}</p>
                  </div>
                </div>
              ))}
            </div>

            <a 
              href="/benefits" 
              class="inline-flex items-center gap-2 text-brand-400 font-semibold hover:text-brand-300 transition-colors"
            >
              See all benefits
              <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3" />
              </svg>
            </a>
          </div>

          <!-- Checklist card -->
          <div class="bg-white/5 backdrop-blur-sm rounded-3xl p-8 border border-white/10">
            <h3 class="text-2xl font-bold mb-6">Do you need duct cleaning?</h3>
            <p class="text-slate-400 mb-6">If you answer "yes" to any of these, it's time to call us:</p>
            <ul class="space-y-4">
              {[
                "Anyone in your house has allergies, asthma, or respiratory issues?",
                "Family members suffer from unexplained headaches or congestion?",
                "There's a smoker in the household?",
                "You have pets that live indoors?",
                "Musty or stale odors when the HVAC runs?",
                "Weak airflow from your vents?"
              ].map(item => (
                <li class="flex items-start gap-3">
                  <div class="flex-shrink-0 w-6 h-6 bg-emerald-500/20 rounded flex items-center justify-center mt-0.5">
                    <svg class="w-4 h-4 text-emerald-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
                    </svg>
                  </div>
                  <span class="text-slate-300">{item}</span>
                </li>
              ))}
            </ul>
            <a 
              href="/contact" 
              class="mt-8 w-full inline-flex items-center justify-center gap-2 px-8 py-4 bg-brand-600 text-white font-semibold rounded-xl hover:bg-brand-700 transition-all"
            >
              Schedule Free Inspection
              <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3" />
              </svg>
            </a>
          </div>
        </div>
      </div>
    </section>

    <!-- Testimonials Section -->
    <section class="py-24 bg-slate-50">
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center max-w-2xl mx-auto mb-16">
          <span class="text-brand-600 font-semibold text-sm uppercase tracking-wider">Testimonials</span>
          <h2 class="text-4xl sm:text-5xl font-bold text-slate-900 mt-3 mb-4">
            Trusted by thousands
          </h2>
          <p class="text-lg text-slate-600">
            See what our customers say about working with Carolina Quality Air.
          </p>
        </div>

        <div id="testimonials-grid" class="grid md:grid-cols-3 gap-8">
          <!-- Loading skeleton -->
          <div class="testimonial-skeleton bg-white rounded-2xl p-8 shadow-sm animate-pulse">
            <div class="flex gap-1 mb-4"><div class="w-24 h-5 bg-slate-200 rounded"></div></div>
            <div class="space-y-2 mb-6"><div class="h-4 bg-slate-200 rounded w-full"></div><div class="h-4 bg-slate-200 rounded w-5/6"></div><div class="h-4 bg-slate-200 rounded w-4/6"></div></div>
            <div class="h-4 bg-slate-200 rounded w-1/3"></div>
          </div>
          <div class="testimonial-skeleton bg-white rounded-2xl p-8 shadow-sm animate-pulse hidden md:block">
            <div class="flex gap-1 mb-4"><div class="w-24 h-5 bg-slate-200 rounded"></div></div>
            <div class="space-y-2 mb-6"><div class="h-4 bg-slate-200 rounded w-full"></div><div class="h-4 bg-slate-200 rounded w-5/6"></div><div class="h-4 bg-slate-200 rounded w-4/6"></div></div>
            <div class="h-4 bg-slate-200 rounded w-1/3"></div>
          </div>
          <div class="testimonial-skeleton bg-white rounded-2xl p-8 shadow-sm animate-pulse hidden md:block">
            <div class="flex gap-1 mb-4"><div class="w-24 h-5 bg-slate-200 rounded"></div></div>
            <div class="space-y-2 mb-6"><div class="h-4 bg-slate-200 rounded w-full"></div><div class="h-4 bg-slate-200 rounded w-5/6"></div><div class="h-4 bg-slate-200 rounded w-4/6"></div></div>
            <div class="h-4 bg-slate-200 rounded w-1/3"></div>
          </div>
        </div>

        <script>
          async function loadTestimonials() {
            try {
              const res = await fetch('https://cqa-public-api.bennyforeman1.workers.dev/testimonials');
              const data = await res.json();
              const grid = document.getElementById('testimonials-grid');
              if (!data.length) return;
              grid.innerHTML = data.slice(0, 3).map(t => `
                <div class="bg-white rounded-2xl p-8 shadow-sm hover:shadow-lg transition-shadow">
                  <div class="flex gap-1 mb-4">${Array(t.rating).fill().map(() => `<svg class="w-5 h-5 text-amber-400" fill="currentColor" viewBox="0 0 20 20"><path d="M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z"/></svg>`).join('')}</div>
                  <p class="text-slate-600 mb-6 leading-relaxed">"${t.text}"</p>
                  <div><div class="font-semibold text-slate-900">${t.name}</div><div class="text-sm text-slate-500">${t.location_type}</div></div>
                </div>
              `).join('');
            } catch (e) { console.error('Failed to load testimonials:', e); }
          }
          document.addEventListener('DOMContentLoaded', loadTestimonials);
        </script>

        <div class="text-center mt-12">
          <a 
            href="/testimonials" 
            class="inline-flex items-center gap-2 text-brand-600 font-semibold hover:text-brand-700 transition-colors"
          >
            Read more reviews
            <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3" />
            </svg>
          </a>
        </div>
      </div>
    </section>

    <!-- CTA Section -->
    <section class="py-24 bg-brand-600 relative overflow-hidden">
      <div class="absolute inset-0 opacity-10">
        <div class="absolute inset-0" style="background-image: url('data:image/svg+xml,%3Csvg width=\'60\' height=\'60\' viewBox=\'0 0 60 60\' xmlns=\'http://www.w3.org/2000/svg\'%3E%3Cg fill=\'none\' fill-rule=\'evenodd\'%3E%3Cg fill=\'%23ffffff\' fill-opacity=\'1\'%3E%3Cpath d=\'M36 34v-4h-2v4h-4v2h4v4h2v-4h4v-2h-4zm0-30V0h-2v4h-4v2h4v4h2V6h4V4h-4zM6 34v-4H4v4H0v2h4v4h2v-4h4v-2H6zM6 4V0H4v4H0v2h4v4h2V6h4V4H6z\'/%3E%3C/g%3E%3C/g%3E%3C/svg%3E');"></div>
      </div>
      
      <div class="relative max-w-4xl mx-auto px-4 sm:px-6 lg:px-8 text-center">
        <h2 class="text-4xl sm:text-5xl font-bold text-white mb-6">
          Ready to breathe easier?
        </h2>
        <p class="text-xl text-white/80 mb-10 max-w-2xl mx-auto">
          Get a free quote from Eastern NC's most trusted NADCA-certified duct cleaning experts. 
          We serve Raleigh, Cary, Greenville, Wilmington, and surrounding areas.
        </p>
        <div class="flex flex-col sm:flex-row gap-4 justify-center">
          <a 
            href="/contact" 
            class="inline-flex items-center justify-center gap-2 px-8 py-4 bg-white text-brand-600 font-semibold rounded-full hover:bg-slate-100 transition-all hover:shadow-xl"
          >
            Get Free Quote
            <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3" />
            </svg>
          </a>
          <a 
            href="tel:+12523217447"
            class="inline-flex items-center justify-center gap-2 px-8 py-4 bg-transparent text-white font-semibold rounded-full border-2 border-white/30 hover:bg-white/10 transition-all"
